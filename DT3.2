#include<stdio.h>
 
int getProduct(int a, int b, int *product)
{
    *product = a * b;
    return a*b;
}
 
int main()
{
    int a, b, product;
     
    printf("Enter two integers\n");
    scanf("%d %d", &a, &b);
     
    product = getProduct(a, b, &product);
     
    printf("Product = %d", product);
  
    return 0;
}
