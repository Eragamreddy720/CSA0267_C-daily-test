#include<stdio.h>
int main()
{
	float a,b,c;
	char s;
	printf("calculator progrm");
	printf("\n+add\n-sub\n*mul\n/div\n");
	printf("Enter a operator symbol: ");
	scanf("%c",&s);
	printf("Enter a,b values: ");
	scanf("%f%f",&a,&b);
	switch(s)
{
	case '+':
	c=a+b;
	break;
	case '-':
	c=a-b;
	break;
	case '*':
	c=a*b;
	break;
	case '/':
	c=a/b; 
	break;
}
printf("The result is %f",c);
return 0;
}
